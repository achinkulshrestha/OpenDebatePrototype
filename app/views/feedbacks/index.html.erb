<p id="notice"><%= notice %></p>

<h1>Listing Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Debate</th>
      <th>Rator</th>
      <th>Rated</th>
      <th>Explain</th>
      <th>Listen</th>
      <th>Interest</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.debate_id %></td>
        <td><%= feedback.rator_id %></td>
        <td><%= feedback.rated_id %></td>
        <td><%= feedback.explain %></td>
        <td><%= feedback.listen %></td>
        <td><%= feedback.interest %></td>
        <td><%= feedback.comments %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
